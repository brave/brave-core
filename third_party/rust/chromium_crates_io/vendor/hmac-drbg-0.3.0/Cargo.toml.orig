[package]
name = "hmac-drbg"
version = "0.3.0"
authors = ["Wei Tang <hi@that.world>"]
description = "Pure Rust implementation of Hmac DRBG."
license = "Apache-2.0"
edition = "2018"

[dependencies]
digest = "0.9"
generic-array = "0.14"
hmac = "0.8"

[dev-dependencies]
sha2 = "0.9"
hex = "0.4"
typenum = "1.12"
serde = { version = "1", features = ["derive"] }
serde_json = "1"
