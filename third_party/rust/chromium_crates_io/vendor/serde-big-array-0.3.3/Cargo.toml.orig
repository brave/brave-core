[package]
name = "serde-big-array"
version = "0.3.3"
authors = ["est31 <MTest31@outlook.com>", "David Tolnay <dtolnay@gmail.com>"]
license = "MIT OR Apache-2.0"
description = "Big array helper for serde."
documentation = "https://docs.rs/serde-big-array"
repository = "https://github.com/est31/serde-big-array"
readme = "README.md"
edition = "2018"

[dependencies]
serde = { version = "1.0", default-features = false }

[dev-dependencies]
serde_derive = "1.0"
serde_json = "1.0"

[features]
const-generics = []
