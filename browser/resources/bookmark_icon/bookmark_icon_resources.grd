<?xml version="1.0" encoding="utf-8"?>
<grit latest_public_release="0" current_release="1">
  <outputs>
    <output filename="grit/bookmark_icon_resources.h" type="rc_header" context="default_100_percent">
      <emit emit_type='prepend'></emit>
    </output>
    <output filename="grit/bookmark_icon_resources_map.cc" type="resource_map_source" context="default_100_percent" />
    <output filename="grit/bookmark_icon_resources_map.h" type="resource_map_header" context="default_100_percent" />
    <output filename="bookmark_icon_resources_100_percent.pak" type="data_package" context="default_100_percent" />
    <output filename="bookmark_icon_resources_200_percent.pak" type="data_package" context="default_200_percent" />
  </outputs>
  <release seq="1">
    <structures fallback_to_low_resolution="true">
      <if expr="is_win">
        <structure type="chrome_scaled_image" name="IDR_BRAVE_BOOKMARK_FOLDER_CLOSED_DARK" file="brave_bookmark_folder_closed_win_dark.png" />
        <structure type="chrome_scaled_image" name="IDR_BRAVE_BOOKMARK_FOLDER_CLOSED_LIGHT" file="brave_bookmark_folder_closed_win_light.png" />
      </if>
      <if expr="is_linux">
        <structure type="chrome_scaled_image" name="IDR_BRAVE_BOOKMARK_FOLDER_CLOSED_DARK" file="brave_bookmark_folder_closed_lin_dark.png" />
        <structure type="chrome_scaled_image" name="IDR_BRAVE_BOOKMARK_FOLDER_CLOSED_LIGHT" file="brave_bookmark_folder_closed_lin_light.png" />
      </if>
      <if expr="not is_win and not is_linux">
        <structure type="chrome_scaled_image" name="IDR_BRAVE_BOOKMARK_FOLDER_CLOSED_DARK" file="brave_bookmark_folder_closed_dark.png" />
        <structure type="chrome_scaled_image" name="IDR_BRAVE_BOOKMARK_FOLDER_CLOSED_LIGHT" file="brave_bookmark_folder_closed_light.png" />
      </if>
    </structures>
  </release>
</grit>

