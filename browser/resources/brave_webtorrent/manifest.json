{
  "version": "1.0.0",
  "name": "Brave Webtorrent",
  "manifest_version": 2,
  "description": "Webtorrent extension",
  "background" : {
    "scripts": ["brave_webtorrent_background.bundle.js"],
    "persistent": true
  },
  "permissions": ["tabs", "windows"],
  "sockets": {
    "udp": {
      "send": "*",
      "bind": "*"
    },
    "tcp": {
      "connect": "*"
    },
    "tcpServer": {
      "listen": "*:*"
    }
  },
  "key": ""
}
