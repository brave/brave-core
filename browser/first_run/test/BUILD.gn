source_set("unit_tests") {
  testonly = true
  sources = [ "first_run_unittest.cc" ]

  deps = [
    "//base",
    "//brave/browser/first_run",
    "//brave/browser/metrics",
    "//chrome/browser",
    "//testing/gtest",
  ]

  if (is_mac) {
    deps += [ "//chrome/browser/ui" ]
  }
}
