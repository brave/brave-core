<translation id="4659866260728515512">ಈ ಟ್ಯಾಬ್‌ಗಳು ಹೆಚ್ಚುವರಿ ಮಾಹಿತಿಯ ಮೂಲಗಳನ್ನು ಬಳಸುತ್ತಿವೆ. ನಿಮ್ಮ ಕಾರ್ಯಕ್ಷಮತೆಯನ್ನು ಸುಧಾರಿಸಲು, ಅವುಗಳನ್ನು ನಿಷ್ಕ್ರಿಯಗೊಳಿಸಲು Brave ಅನ್ನು ಅನುಮತಿಸಿ.</translation>
<translation id="6806912298071949921">ನಿಮ್ಮ Brave ಪ್ರೊಫೈಲ್ ಅನ್ನು ಕಸ್ಟಮೈಸ್ ಮಾಡಿ</translation>
<translation id="3792238845239580301">ನಿಮ್ಮ Brave ಪ್ರೊಫೈಲ್ ಅನ್ನು ಹೆಸರಿಸಿ</translation>
<translation id="5874208157501051620">Brave ಗೆ ಸೈನ್ ಇನ್ ಮಾಡಲು ಎಕ್ಸ್‌ಟೆನ್ಶನ್ ನಿಮ್ಮನ್ನು ಕೇಳುತ್ತದೆ</translation>
<translation id="3561569806301739367"><ph name="FILE_NAME"/> ಅಪಾಯಕಾರಿಯಾಗಿರಬಹುದು, ಹಾಗಾಗಿ Brave ಅದನ್ನು ನಿರ್ಬಂಧಿಸಿದೆ.</translation>
<translation id="3205074487970075628">Brave ನಿಮ್ಮ ಕ್ಯಾಮರಾ ಬಳಸುತ್ತಿದೆ.</translation>
<translation id="6327105987658262776">ಯಾವುದೇ ಅಪ್‌ಡೇಟ್‌‌ ಲಭ್ಯವಿಲ್ಲ.</translation>
<translation id="1819119299840995223">Brave ನಲ್ಲಿ ಬೇರೆ ಕಾರ್ಯಾಚರಣೆಯು ಪ್ರಗತಿಯಲ್ಲಿದೆ. ದಯವಿಟ್ಟು ನಂತರ ಮತ್ತೆ ಪ್ರಯತ್ನಿಸಿ.</translation>
<translation id="8616874628011103667">ನಿಮ್ಮ Brave ಖಾತೆಯ ಮೂಲಕ ಮತ್ತು ಹಂಚಿದ ಕಂಪ್ಯೂಟರ್‌ಗಳಲ್ಲಿ Brave ಬಳಕೆಯು ಇದೀಗ ತುಂಬಾ ಸುಲಭವಾಗಿದೆ.</translation>
<translation id="2727290586877408122">ನಿಮ್ಮ ಬ್ರೌಸಿಂಗ್ ಅನುಭವವನ್ನು ಸುಧಾರಿಸಲು ಮತ್ತು ಕಾರ್ಯಕ್ಷಮತೆಯನ್ನು ಉತ್ತಮಗೊಳಿಸಲು, Brave ಈ ಟ್ಯಾಬ್ ಅನ್ನು ನಿಷ್ಕ್ರಿಯಗೊಳಿಸಬಹುದು.</translation>
<translation id="4264606872840545968">Brave ಶಾರ್ಟ್‌ಕಟ್</translation>
<translation id="6890616081898535725">ಈ ಫೈಲ್ ಅನ್ನು ಸಾಮಾನ್ಯ ರೀತಿಯಲ್ಲಿ ಡೌನ್‌ಲೋಡ್ ಮಾಡದ ಕಾರಣ Brave ಈ ಡೌನ್‌ಲೋಡ್ ಅನ್ನು ನಿರ್ಬಂಧಿಸಿದೆ ಹಾಗೂ ಇದು ಅಪಾಯಕಾರಿಯಾಗಿರಬಹುದು</translation>
<translation id="335632367599648646">Brave ಅನ್‌ಇನ್‌ಸ್ಟಾಲ್ ಮಾಡಲು ನೀವು ಖಚಿತವಾಗಿ ಬಯಸುವಿರಾ?</translation>
<translation id="6375219077595103062">Password Manager ಗೆ ಶಾರ್ಟ್‌ಕಟ್ ಸೇರಿಸಿ</translation>
<translation id="4867486350347355096"><ph name="BEGIN_BOLD"/>ನಾವು ಈ ಡೇಟಾವನ್ನು ಹೇಗೆ ಬಳಸುತ್ತೇವೆ:<ph name="END_BOLD"/> Brave ನಿಮ್ಮ ಆಸಕ್ತಿಗಳನ್ನು ಅಂದಾಜು ಮಾಡಬಲ್ಲದು. ಆನಂತರ, ನೀವು ಭೇಟಿ ನೀಡುವ ಸೈಟ್ ನಿಮಗೆ ಕಾಣಿಸುವ ಜಾಹೀರಾತುಗಳನ್ನು ವೈಯಕ್ತಿಕಗೊಳಿಸಲು ನಿಮ್ಮ ಆಸಕ್ತಿಗಳನ್ನು ತೋರಿಸುವಂತೆ Brave ಅನ್ನು ಕೇಳಬಹುದು.</translation>
<translation id="3256100650978957522">ಕಂಪ್ಯೂಟರ್ ಸ್ಕ್ರೀನ್‌ನೊಳಗೆ Brave ಲೋಗೋ.</translation>
<translation id="6373767849825830651">Brave ಒಂದು ವೆಬ್ ಬ್ರೌಸರ್ ಆಗಿದ್ದು ವೆಬ್‌ಪುಟಗಳನ್ನು ಮತ್ತು ಅಪ್ಲಿಕೇಶನ್‌ಗಳನ್ನು ವೇಗದೊಂದಿಗೆ ಕಾರ್ಯನಿರ್ವಹಿಸುತ್ತದೆ. ಇದು ವೇಗವಾದ, ಸ್ಥಿರವಾದ, ಮತ್ತು ಬಳಸಲು ಸುಲಭವಾಗಿದೆ. ಮಾಲ್‌ವೇರ್‌ನೊಂದಿಗೆ ಹೆಚ್ಚು ಸುರಕ್ಷಿತವಾಗಿ ಹಾಗೂ Brave ನಲ್ಲಿ ವಿನ್ಯಾಸಗೊಳಿಸಲಾದ ಫಿಶಿಂಗ್ ಸುರಕ್ಷೆಯೊಂದಿಗೆ ವೆಬ್ ಅನ್ನು ಬ್ರೌಸ್ ಮಾಡಿ.</translation>
<translation id="2139300032719313227">ChromeOS ಮರುಪ್ರಾರಂಭಿಸಿ</translation>
<translation id="638302211119176182">ನೀವು ಭೇಟಿ ನೀಡುವ ಸಾಧ್ಯತೆಯಿರುವ ಇನ್ನೂ ಹೆಚ್ಚಿನ ಪುಟಗಳನ್ನು Brave ಪ್ರಿಲೋಡ್ ಮಾಡುತ್ತದೆ, ಇದರಿಂದ ನೀವು ಅವುಗಳಿಗೆ ಭೇಟಿ ನೀಡಿದಾಗ ಅವುಗಳು ಇನ್ನಷ್ಟು ವೇಗವಾಗಿ ಲೋಡ್ ಆಗುತ್ತವೆ</translation>
<translation id="890319829408077946">ಆಫ್ • ಈ ಎಕ್ಸ್‌ಟೆನ್ಶನ್ ಎಲ್ಲಿಂದ ಬರುತ್ತದೆ ಎಂಬುದನ್ನು ಪರಿಶೀಲಿಸಲು Brave ಗೆ ಸಾಧ್ಯವಿಲ್ಲ</translation>
<translation id="1684121725014454063">Brave ಪ್ರೊಫೈಲ್‌ಗಳಿಗೆ ಸುಸ್ವಾಗತ</translation>
<translation id="7906693299331092358">Brave ನಿಂದ ಖಾತೆಯನ್ನು ತೆಗೆದುಹಾಕಿ</translation>
<translation id="2675662665675552980">ನೀವು Brave ಪ್ರಾರಂಭಿಸಿದಾಗ ತೋರಿಸಬೇಕಾದ ಪುಟವನ್ನು ಕೂಡಾ ಇದು ನಿಯಂತ್ರಿಸುತ್ತದೆ.</translation>
<translation id="648319183876919572">ಅಪಾಯಕಾರಿ ವೆಬ್‌ಸೈಟ್‌ಗಳು ಮತ್ತು ಡೌನ್‌ಲೋಡ್‌ಗಳ ವಿರುದ್ಧ ನಿಮ್ಮನ್ನು ರಕ್ಷಿಸಲು ವರ್ಧಿತ ಸುರಕ್ಷಿತ ಬ್ರೌಸಿಂಗ್ ಹೆಚ್ಚಿನ ಸಹಾಯ ಮಾಡುತ್ತದೆ</translation>
<translation id="5590013613412457132">Brave ಅನ್‌ಇನ್‌ಸ್ಟಾಲ್ ಮಾಡಿ</translation>
<translation id="7185585387858242335">Brave ನಲ್ಲಿ ಶಾರ್ಟ್‌ಕಟ್‌ಗಳು ತೆರೆದಿರುತ್ತವೆ</translation>
<translation id="5324409579559361527">ನೀವು ಸೈನ್ ಇನ್ ಆಗಿರುವಾಗ, Brave ನಲ್ಲಿ ನಿಮ್ಮ Brave ಖಾತೆಯಲ್ಲಿನ ಪಾಸ್‌ವರ್ಡ್‌ಗಳು ಹಾಗೂ ಇನ್ನಷ್ಟನ್ನು ನೀವು ಬಳಸಬಹುದು. ನೀವು ಯಾವುದೇ ಸಮಯದಲ್ಲಿ ಸೆಟ್ಟಿಂಗ್‌ಗಳಲ್ಲಿ ಇದನ್ನು ಬದಲಾಯಿಸಬಹುದು.</translation>
<translation id="3617744907966785605">Brave ಅನ್ನು <ph name="TIMEOUT_DURATION"/> ಅವಧಿಯವರೆಗೆ ಬಳಸದಿದ್ದರೆ ನಿಮ್ಮ ಸಂಸ್ಥೆ ಅದನ್ನು ಮುಚ್ಚುತ್ತದೆ. ಬ್ರೌಸಿಂಗ್ ಡೇಟಾವನ್ನು ಅಳಿಸಲಾಗಿದೆ. ಇದು ಇತಿಹಾಸ, ಆಟೋಫಿಲ್ ಮತ್ತು ಡೌನ್‌ಲೋಡ್‌ಗಳನ್ನು ಒಳಗೊಂಡಿರಬಹುದು.</translation>
<translation id="8646609321956191980">ಪ್ರತಿಯೊಂದು ಪ್ರೊಫೈಲ್ ಬುಕ್‌ಮಾರ್ಕ್‌ಗಳು, ಇತಿಹಾಸ, ಪಾಸ್‌ವರ್ಡ್‌ಗಳು ಹಾಗೂ ಇನ್ನಷ್ಟವುಗಳಂತಹ ತನ್ನ ಸ್ವಂತ Brave ಮಾಹಿತಿಯನ್ನು ಒಳಗೊಂಡಿರುತ್ತವೆ</translation>
<translation id="3426839233411581346">ನಿಷ್ಕ್ರಿಯ ಟ್ಯಾಬ್‌ಗಳಿಂದ Brave ಮೆಮೊರಿಯನ್ನು ಮುಕ್ತಗೊಳಿಸುತ್ತದೆ. ಇದರಿಂದ ಸಕ್ರಿಯ ಟ್ಯಾಬ್‌ಗಳು ಹಾಗೂ ಇತರ ಆ್ಯಪ್‌ಗಳಿಗೆ ಕಂಪ್ಯೂಟರ್‌ನ ಹೆಚ್ಚಿನ ಸಂಪನ್ಮೂಲಗಳು ದೊರೆಯುತ್ತವೆ ಹಾಗೂ Brave ವೇಗವಾಗಿ ಕೆಲಸ ಮಾಡುತ್ತದೆ. ನಿಮ್ಮ ನಿಷ್ಕ್ರಿಯ ಟ್ಯಾಬ್‌ಗಳಿಗೆ ನೀವು ಹಿಂತಿರುಗಿದಾಗ ಅವು ಸ್ವಯಂಚಾಲಿತವಾಗಿ ಮತ್ತೆ ಸಕ್ರಿಯವಾಗುತ್ತವೆ.</translation>
<translation id="6756656512757374876"><ph name="BEGIN_LINK"/>Brave ನಿಮ್ಮ ಡೇಟಾವನ್ನು ಹೇಗೆ ಖಾಸಗಿಯಾಗಿ ಇರಿಸುತ್ತದೆ<ph name="END_LINK"/> ಎಂಬುದರ ಕುರಿತು ಇನ್ನಷ್ಟು ತಿಳಿಯಿರಿ</translation>
<translation id="2535436634051789953">ಪಾಸ್‌ವರ್ಡ್‌ಗಳನ್ನು ವೀಕ್ಷಿಸಲು ನೀವು ಇನ್ನೊಂದು Brave ಪ್ರೊಫೈಲ್‌ಗೆ ಬದಲಾಯಿಸಬಹುದು</translation>
<translation id="2312399718665761438">ಕಾಗುಣಿತ ದೋಷಗಳನ್ನು ಸರಿಪಡಿಸಲು, ನೀವು ಪಠ್ಯ ಕ್ಷೇತ್ರಗಳಲ್ಲಿ ಟೈಪ್ ಮಾಡುವ ಪಠ್ಯವನ್ನು Brave, Brave ಗೆ ಕಳುಹಿಸುತ್ತದೆ</translation>
<translation id="3507835409099344374">Brave ನೋಟಿಫಿಕೇಶನ್ ಸೆಟ್ಟಿಂಗ್‌ಗಳಿಗೆ ಹೋಗಿ</translation>
<translation id="9156965752763025780">ನೀವು ಮುಂದಿನ ಬಾರಿ Brave ಅನ್ನು ಮರುಪ್ರಾರಂಭಿಸಿದಾಗ ನಿಮ್ಮ ಬದಲಾವಣೆಗಳು ಕಾರ್ಯಗತಗೊಳ್ಳುತ್ತವೆ.</translation>
<translation id="245693535809364497">ನಿಮ್ಮ ಸುತ್ತಮುತ್ತಲಿನ ಪ್ರದೇಶದ 3D ನಕ್ಷೆಯನ್ನು ರಚಿಸಲು Brave ಗೆ ಕ್ಯಾಮರಾ ಅನುಮತಿಯ ಅಗತ್ಯವಿದೆ</translation>
<translation id="5060669762365466104">ಸೈಟ್‌ಗಳು ನೀವು ನಿರೀಕ್ಷಿಸಿದಂತೆ ಬಹುಶಃ ಕಾರ್ಯನಿರ್ವಹಿಸುತ್ತವೆ ಆದರೆ ನೀವು ಎಲ್ಲಾ Brave ವಿಂಡೋಗಳನ್ನು ಮುಚ್ಚಿದ ನಂತರ ನಿಮ್ಮನ್ನು ನೆನಪಿಟ್ಟುಕೊಳ್ಳುವುದಿಲ್ಲ</translation>
<translation id="9210457680074341078">ನಿಮ್ಮ Brave ಖಾತೆಯಲ್ಲಿ ನಿಮ್ಮ ಬುಕ್‌ಮಾರ್ಕ್‌ಗಳನ್ನು ಸಹ ನೀವು ಸೇವ್ ಮಾಡಿದರೆ, ನೀವು Brave ನಲ್ಲಿ ಉತ್ಪನ್ನದ ಬೆಲೆಗಳನ್ನು ಟ್ರ್ಯಾಕ್ ಮಾಡಬಹುದು ಮತ್ತು ಬೆಲೆ ಕಡಿಮೆಯಾದಾಗ ಸೂಚನೆ ಪಡೆಯಬಹುದು</translation>
<translation id="6676384891291319759">ಇಂಟರ್ನೆಟ್ ಅನ್ನು ಪ್ರವೇಶಿಸಿ</translation>
<translation id="1363996462118479832">ಸೈನ್ ಇನ್ ಮಾಡುವಲ್ಲಿ ದೋಷವಿರುವ ಕಾರಣ ನಿಮ್ಮ ಡೇಟಾವನ್ನು ಸಿಂಕ್ ಮಾಡಲು ChromeOS ಗೆ ಸಾಧ್ಯವಾಗುತ್ತಿಲ್ಲ.</translation>
<translation id="5324448127277983819">ನೀವು ನೋಡುವ ಜಾಹೀರಾತುಗಳ ಮೇಲೆ Brave ನಿಮಗೆ ಹೆಚ್ಚಿನ ನಿಯಂತ್ರಣವನ್ನು ನೀಡುತ್ತದೆ ಮತ್ತು ಸೈಟ್‌ಗಳಲ್ಲಿ ನೀವು ವೈಯಕ್ತೀಕರಿಸಿದ ಜಾಹೀರಾತುಗಳನ್ನು ನೋಡಿದಾಗ ನಿಮ್ಮ ಕುರಿತು ಯಾವ ಸೈಟ್‌ಗಳು ತಿಳಿಯಬಹುದು ಎಂಬುದನ್ನು ಮಿತಿಗೊಳಿಸುತ್ತದೆ</translation>
<translation id="1257247502847251159">ಈ ಸೈಟ್‌ಗೆ ಸಂಬಂಧಿಸಿದಂತೆ Brave ಗೆ ಮೈಕ್ರೊಫೋನ್ ಅನುಮತಿಯ ಅಗತ್ಯವಿದೆ</translation>
<translation id="6712881677154121168">ಡೌನ್‌ಲೋಡ್ ದೋಷ: <ph name="DOWNLOAD_ERROR"/>.</translation>
<translation id="1489465648257900784">Brave ನ ಹೊಸ ಆವೃತ್ತಿಯಾಗಿರುವುದರಿಂದ ನಿಮ್ಮ ಪ್ರೊಫೈಲ್ ಅನ್ನು ಬಳಸಲಾಗುವುದಿಲ್ಲ. ಕೆಲವೊಂದು ವೈಶಿಷ್ಟ್ಯಗಳು ಲಭ್ಯವಿಲ್ಲದಿರಬಹುದು. ದಯವಿಟ್ಟು ಬೇರೆ ಒಂದು ಪೊಫೈಲ್ ಡೈರೆಕ್ಟರಿ ಅನ್ನು ನಿರ್ದಿಷ್ಟಪಡಿಸಿ ಅಥವಾ Brave ನ ಹೊಸ ಆವೃತ್ತಿಯನ್ನು ಬಳಸಿ.</translation>
<translation id="1505112054322049777">ವೈಯಕ್ತೀಕರಣ ಮತ್ತು ಇತರ ಫೀಚರ್‌ಗಳನ್ನು ಪಡೆಯಲು, ವೆಬ್ ಮತ್ತು ಆ್ಯಪ್ ಚಟುವಟಿಕೆ ಹಾಗೂ ಲಿಂಕ್ ಮಾಡಲಾದ Brave ಸೇವೆಗಳಲ್ಲಿ Brave ಅನ್ನು ಸೇರಿಸಿ</translation>
<translation id="6666624454809779082">ನಿಮ್ಮ ಕಂಪ್ಯೂಟರ್‌ನಲ್ಲಿನ ಎಲ್ಲ ಬಳಕೆದಾರರಿಗೆ ಈಗಾಗಲೇ Brave ಸ್ಥಾಪಿತವಾಗಿದೆ.</translation>
