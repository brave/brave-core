// This is for EXTERNAL beta testing

#include "Base.xcconfig"

MOZ_BUNDLE_DISPLAY_NAME = BraveBeta

BRAVE_API_KEY = key
BRAVE_SERVICES_KEY = key
BRAVE_URL_SCHEME = brave-beta

// Bundle Identifier
MOZ_BUNDLE_ID = $(BASE_BUNDLE_ID).browser.beta

