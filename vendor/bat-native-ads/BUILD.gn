  import("//build/config/android/rules.gni")
}

dep_base = rebase_path("../..", "//")

config("external_config") {
  visibility = [ ":*" ]
  include_dirs = [
    "include",
    "$target_gen_dir/include",
  ]
}

config("internal_config") {
  visibility = [
    ":*",
    "//brave/components/brave_ads/browser:testutil",
    "//brave/components/brave_ads/test:*",
    "//brave/test:*",
  ]
  include_dirs = [ "src" ]
}

group("bat-native-ads") {
  public_deps = [ ":ads" ]

  data_deps = [ ":bat-native-ads-standalone" ]
}

static_library("bat-native-ads-standalone") {
  # TODO(bridiver) - only export symbols from `include`
  complete_static_lib = true

  output_dir = root_out_dir

  configs -= [ "//build/config/compiler:thin_archive" ]

  defines = [ "STANDALONE_BUILD" ]

  deps = [ ":ads" ]
}

