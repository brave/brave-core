// Cardano RPC structures.
namespace cardano_rpc {
  dictionary Block {
    // uint32_t
    DOMString height;

    // uint64_t
    DOMString slot;

    // uint32_t
    DOMString epoch;
  };

  dictionary EpochParameters {
    // uint64_t
    DOMString min_fee_a;

    // uint64_t
    DOMString min_fee_b;
  };


  dictionary UnspentOutputAsset {
    DOMString unit;
    DOMString quantity;
  };

  dictionary UnspentOutput {
    DOMString tx_hash;

    // uint32_t
    DOMString output_index;

    UnspentOutputAsset[] amount;
  };
};
