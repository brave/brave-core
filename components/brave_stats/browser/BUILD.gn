import("//build/buildflag_header.gni")

declare_args() {
  brave_stats_api_key = ""
}

buildflag_header("buildflags") {
  header = "buildflags.h"
  flags = [ "BRAVE_STATS_API_KEY=\"$brave_stats_api_key\"" ]
}

static_library("browser") {
  sources = [
    "brave_stats_updater_util.cc",
    "brave_stats_updater_util.h",
  ]

  deps = [
    ":buildflags",
    "//base",
    "//components/prefs",
  ]
}
