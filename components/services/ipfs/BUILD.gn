    "ipfs_service_utils.h",
  ]
  if (!is_android) {
    sources += [ "ipfs_service_impl.cc" ]
  } else {
    sources += [ "ipfs_service_impl_android.cc" ]
  }

  deps = [
    "//base",
    "//brave/components/child_process_monitor",
    "//brave/components/services/ipfs/public/mojom",
    "//mojo/public/cpp/bindings",
    "//third_party/re2",
  ]
}
