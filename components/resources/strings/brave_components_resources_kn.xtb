<translation id="723054503945711909">Brave Software Inc. ಜೊತೆಗೆ ನೀವು ಖಾತೆಯನ್ನು ಹೊಂದಿಸಿದಾಗ ನೀವು ಅಪ್‌ಹೋಲ್ಡ್ ಮಾಡಲು ಒದಗಿಸುವ ವೈಯಕ್ತಿಕ ಮಾಹಿತಿಯನ್ನು ಪ್ರಕ್ರಿಯೆಗೊಳಿಸುವುದಿಲ್ಲ, ಸಂಗ್ರಹಿಸುವುದಿಲ್ಲ ಅಥವಾ ಪ್ರವೇಶಿಸುವುದಿಲ್ಲ.</translation>
<translation id="693248854158660756">Brave Rewards ಕುರಿತು</translation>
<translation id="161118570147571596">‘ರಿವಾರ್ಡ್‌ಗಳಿಗೆ ಸೇರ್ಪಡೆಗೊಳಿಸು’ ಕ್ಲಿಕ್ ಮಾಡುವ ಮೂಲಕ, ಇದನ್ನು ನೀವು ಓದಿರುವಿರಿ ಮತ್ತು ಸಮ್ಮತಿಸಿದ್ದೀರಿ ಎಂದು ಸೂಚಿಸುತ್ತೀರಿ</translation>
<translation id="8344084848798035850">ಈ ತಿಂಗಳು ಇಲ್ಲಿಯವರೆಗಿನ ಅಂದಾಜು ಗಳಿಕೆ</translation>
<translation id="5235444069025581717">ಈ ತಿಂಗಳಿನ ಒಟ್ಟು ಸಲಹೆಗಳು ಮತ್ತು ಪಾಯಿಂಟ್‌ಗಳು</translation>
<translation id="1210965535849006928">ಮೈ ರಿವಾರ್ಡ್‌ಗಳು ಅನ್ನು ಕ್ಲೇಮ್ ಮಾಡಿಕೊಳ್ಳಿ</translation>
<translation id="8315514906653279104">ಆನ್ ಮಾಡಲಾಗುತ್ತಿದೆ…</translation>
<translation id="7307847920988946894">ರಿವಾರ್ಡ್ ಗಳನ್ನು ಆನ್ ಮಾಡಿ</translation>
<translation id="662631138774963814">ಉದಾರ ಭಾವನೆಯಿದೆಯೇ?</translation>
<translation id="3264975266761862670">ಹಣ ಗಳಿಸಲು ಪ್ರಾರಂಭಿಸಲು ಸಿದ್ಧರಿದ್ದೀರಾ?</translation>
<translation id="2542495049135486284">Brave Rewards ಮತ್ತೆ ಪ್ರಯತ್ನಿಸಿ ಮತ್ತು ನೀವು ಹಣ ಗಳಿಸಿದಾಗ ಅದನ್ನು ಮುಂದೆ ಪಾವತಿಸಿ!</translation>
<translation id="7194640987408033097">Brave Rewards ಆನ್ ಮಾಡಿ ಮತ್ತು ಗೌಪ್ಯತೆ ಗೌರವಿಸುವ ಜಾಹೀರಾತುಗಳನ್ನು ವೀಕ್ಷಿಸುವುದಕ್ಕಾಗಿ ಹಣ ಪಡೆಯಿರಿ.</translation>
<translation id="8568666589217601917">ಮುಂದುವರಿಯಿರಿ!</translation>
<translation id="764235488372037377">ಜಾಹೀರಾತುಗಳಿಂದ ಗಳಿಸಿದ ನಿಮ್ಮ ಬಹುಮಾನಗಳು ಇಲ್ಲಿವೆ</translation>
<translation id="7091215606170476659">ನೀವು Brave ನಿಂದ UGP ಗ್ರ್ಯಾಂಟ್ ಅನ್ನು ಹೊಂದಿದ್ದೀರಿ.</translation>
<translation id="1181037720776840403">ತೆಗೆದುಹಾಕಿ</translation>
<translation id="7885157704277298658">ನಕಲಿಸಲಾಗಿದೆ!</translation>
<translation id="5379640681913450928">ಪದ ಎಣಿಕೆ:</translation>
<translation id="2740531572673183784">ಸರಿ</translation>
<translation id="1880258206282877288">ನಿಮ್ಮ ಹೊಸ ಸಾಧನ ಸಂಪರ್ಕವನ್ನು ಖಚಿತಪಡಿಸಲು ಸಿಂಕ್ ಇನ್ನೂ ನಿರೀಕ್ಷಿಸುತ್ತಿದೆ. ನೀವು ಈ ವಿಂಡೋವನ್ನು ಮುಚ್ಚಿದರೆ, ನಿಮ್ಮ ಸಿಂಕ್ ಚೈನ್ ಅನ್ನು ಮರುಹೊಂದಿಸಲಾಗುತ್ತದೆ ಮತ್ತು ನೀವು ಆರಂಭಿಸಬೇಕಾಗುತ್ತದೆ.</translation>
<translation id="5351846961038505462">ಹೇಗಾದರೂ ಮುಚ್ಚಿ</translation>
<translation id="1555452132857939218">ಈ ಸಿಂಕ್ ಚೈನ್‌ನಿಂದ</translation>
<translation id="610118613261910287">ಕೋಡ್ ಪದಗಳನ್ನು ವೀಕ್ಷಿಸಿ</translation>
<translation id="8588535459744752615">ಸಾಧನಕ್ಕಾಗಿ ಹುಡುಕಲಾಗುತ್ತಿದೆ</translation>
<translation id="6406506848690869874">ಸಿಂಕ್</translation>
<translation id="28332161031213248">ಕೆಳಗಿನ ಸಾಧನ ಪಟ್ಟಿಯು ನಿಮ್ಮ ಸಿಂಕ್ ಚೈನ್‌ನಲ್ಲಿರುವ ಎಲ್ಲಾ ಸಾಧನಗಳನ್ನು ಹೊಂದಿದೆ. ಇತರ ಯಾವುದೇ ಸಾಧನದಿಂದ ಪ್ರತಿಯೊಂದು ಸಾಧನವನ್ನು ನಿರ್ವಹಿಸಬಹುದು.</translation>
<translation id="8153617760825732120">ಸಾಧನದ ಹೆಸರು</translation>
<translation id="4241512165288542484">(ಈ ಸಾಧನ)</translation>
<translation id="1415578175043962034">ದಿನಾಂಕ ಸೇರಿಸಲಾಗಿದೆ</translation>
<translation id="1061533051865606843">ಹೊಸ ಸಾಧನವನ್ನು ಸೇರಿಸಿ</translation>
<translation id="8780295742023331191">ಸಿಂಕ್ ಕೋಡ್ ವೀಕ್ಷಿಸಿ</translation>
<translation id="1755198127524719638">ಸಿಂಕ್ ಸೆಟ್ಟಿಂಗ್‌ಗಳು</translation>
<translation id="7558480201297099157">ನೀವು ಸಾಧನಗಳ ನಡುವೆ ಯಾವ ಮಾಹಿತಿಯನ್ನು ಸಿಂಕ್ ಮಾಡಲು ಬಯಸುತ್ತೀರಿ ಎಂಬುದನ್ನು ನಿರ್ವಹಿಸಿ. ಈ ಸೆಟ್ಟಿಂಗ್‌ಗಳು ಕೇವಲ ಈ ಸಾಧನದ ಮೇಲೆ ಪರಿಣಾಮ ಬೀರುತ್ತವೆ.</translation>
<translation id="8820817407110198400">ಬುಕ್‌ಮಾರ್ಕ್‌ಗಳು</translation>
<translation id="5591129654353352557">ಉಳಿಸಿರುವ ಸೈಟ್ ಸೆಟ್ಟಿಂಗ್‌ಗಳು</translation>
<translation id="7859704718976024901">ಬ್ರೌಸಿಂಗ್ ಇತಿಹಾಸ</translation>
<translation id="2339730291919608488">ಸಿಂಕ್ ಚೈನ್ ತೊರೆಯಿರಿ</translation>
<translation id="4332558599726753990">ಸಿಂಕ್ ಸೆಟಪ್</translation>
<translation id="298546024891487702">ಪ್ರಾರಂಭಿಸಲು, ನೀವು ಸಿಂಕ್ ಮಾಡಲು ಯೋಜಿಸಿರುವಂತಹ ಎಲ್ಲಾ ಸಾಧನಗಳಲ್ಲಿ ನೀವು Brave ಇನ್‌ಸ್ಟಾಲ್ ಮಾಡಿರಬೇಕು. ಅವನ್ನು ಜೊತೆಯಾಗಿ ಚೈನ್ ಮಾಡಲು, ನಿಮ್ಮ ಎಲ್ಲಾ ಸಾಧನಗಳನ್ನು ಜೊತೆಯಾಗಿ ಸುಭದ್ರವಾಗಿ ಲಿಂಕ್ ಮಾಡಲು ನೀವು ಬಳಸುವಂತಹ ಸಿಂಕ್ ಚೈನ್ ಅನ್ನು ಆರಂಭಿಸಿ.</translation>
<translation id="6200528822592747640">ಹೊಸ ಸಿಂಕ್ ಚೈನ್ ಅನ್ನು ಪ್ರಾರಂಭಿಸಿ</translation>
<translation id="5184726904895809379">ನಾನು ಸಿಂಕ್ ಕೋಡ್ ಹೊಂದಿದ್ದೇನೆ</translation>
<translation id="8584803368494775413">ಸಿಂಕ್ ಕೋಡ್ ದೃಢಪಡಿಸಿ</translation>
<translation id="5373825572723282843">ಸಿಂಕ್ ಕೋಡ್ ನಮೂದಿಸಿ</translation>
<translation id="5734745018569675530">ಕೆಳಗಿನ ಫಾರ್ಮ್‌ನಲ್ಲಿ ನೀವು ಪೂರೈಸಿದ ಸಿಂಕ್ ಚೈನ್ ಕೋಡ್ ಪದಗಳನ್ನು ಟೈಪ್ ಮಾಡಿ.</translation>
<translation id="8348871245305371520">ಎಲ್ಲಾ ಸಾಧನಗಳಲ್ಲಿ ಸ್ಥಳೀಯ ಸಾಧನ ಡೇಟಾ ಹಾಗೆಯೇ ಇರುತ್ತದೆ. ಈ ಸಿಂಕ್ ಚೈನ್‌ನಲ್ಲಿರುವ ಇತರೆ ಸಾಧನಗಳು ಸಿಂಕ್ ಆಗಿಯೇ ಇರುತ್ತವೆ.</translation>
<translation id="6388211659394268749">ಮತ್ತೊಮ್ಮೆ ಸಿಂಕ್ ಚೈನ್ ಅನ್ನು ಸೇರಿಸಲು, ಆರಿಸಿ “ ಸಿಂಕ್ ಚೈನ್ ಕೋಡ್ ನಮೂದಿಸಿ”.</translation>
<translation id="6500099041876719317">ಗಮನಿಸಿ: ಈ ಸಿಂಕ್ ಚೈನ್‌ನಿಂದ ಈ ಸಾಧನವನ್ನು ತೆಗೆದುಹಾಕುವುದರಿಂದ ಈ ಹಿಂದೆ ಸಿಂಕ್ ಮಾಡಿದ ಡೇಟಾವನ್ನು ಸಾಧನದಿಂದ ತೆರವುಗೊಳಿಸುವುದಿಲ್ಲ.</translation>
<translation id="6647265259633328698">ಎಚ್ಚರಿಕೆ!</translation>
<translation id="7226983081288498250">ತೆಗೆದುಹಾಕಲಾಗುತ್ತಿದೆ</translation>
<translation id="1810742624536999850">ಈ ಸಿಂಕ್ ಚೈನ್ ಅನ್ನು ಅಳಿಸುತ್ತೇವೆ.</translation>
<translation id="3263262632668102801">ಈ ಸಾಧನದಲ್ಲಿನ ನಿಮ್ಮ ಸ್ಥಳೀಯ ಡೇಟಾವನ್ನು ಇರಿಸಿಕೊಳ್ಳಲಾಗುತ್ತದೆ ಆದರೆ Brave’ ನ ಸಿಂಕ್ ಕ್ಯಾಶ್‌ನಲ್ಲಿರುವ ಎಲ್ಲಾ ಡೇಟಾವನ್ನು ಅಳಿಸಲಾಗುತ್ತದೆ.</translation>
<translation id="9209629396585743369">ನೀವು ಹೊಸ ಸಿಂಕ್ ಚೈನ್ ಅನ್ನು ಪ್ರಾರಂಭಿಸಬಹುದು ಅಥವಾ ಅದರ ಸಿಂಕ್ ಚೈನ್ ಕೋಡ್ ಅನ್ನು ನಮೂದಿಸುವ ಮೂಲಕ ಹೊಸ ಸಾಧನದೊಂದಿಗೆ ಸಿಂಕ್ ಮಾಡಬಹುದು.</translation>
<translation id="8258002508340330928">ನೀವು ಖಚಿತವಾಗಿರುವಿರಾ?</translation>
<translation id="3407602902101449803">ಚೈನ್ QR ಕೋಡ್ ಅನ್ನು ಸಿಂಕ್ ಮಾಡಿ</translation>
<translation id="8369686073965491869">ನಿಮ್ಮ ಮೊಬೈಲ್ ಸಾಧನದಲ್ಲಿ, ಸೆಟ್ಟಿಂಗ್‌ಗಳ ಫಲಕದಲ್ಲಿ Brave ಸಿಂಕ್‌ಗೆ ನ್ಯಾವಿಗೇಟ್ ಮಾಡಿ ಮತ್ತು ಬಟನ್ ಕ್ಲಿಕ್ ಮಾಡಿ</translation>
<translation id="6527776448555649991">“ಸಿಂಕ್ ಕೋಡ್ ಅನ್ನು ಸ್ಕ್ಯಾನ್ ಮಾಡಿ”.</translation>
<translation id="1400862780574843709">ಕೆಳಗಿನ QR ಕೋಡ್ ಅನ್ನು ಸ್ಕ್ಯಾನ್ ಮಾಡಲು ನಿಮ್ಮ ಕ್ಯಾಮೆರಾವನ್ನು ಬಳಸಿ.</translation>
<translation id="5086223971958279691">ಚೈನ್ ಕೋಡ್ ಅನ್ನು ಸಿಂಕ್ ಮಾಡಿ</translation>
<translation id="2899261497479389619">ನಿಮ್ಮ ಟಾರ್ಗೆಟ್ ಕಂಪ್ಯೂಟರ್‌ನಲ್ಲಿ, ಸೆಟ್ಟಿಂಗ್‌ಗಳಲ್ಲಿ Brave ಸಿಂಕ್‌ಗೆ ನ್ಯಾವಿಗೇಟ್ ಮಾಡಿ ಮತ್ತು ಬಟನ್ ಅನ್ನು ಕ್ಲಿಕ್ ಮಾಡಿ</translation>
<translation id="4827439149833241990">“ನಾನು ಸಿಂಕ್ ಕೋಡ್ ಹೊಂದಿದ್ದೇನೆ”.</translation>
<translation id="24067401253841795">ಕೆಳಗೆ ತೋರಿಸಲಾದ ಸಿಂಕ್ ಮಾಡಲಾದ ಸರಣಿ ಕೋಡ್ ಪದಗಳನ್ನು ನಮೂದಿಸಿ. ಈ ಕೋಡ್ ಅನ್ನು ಪಾಸ್‌ವರ್ಡ್‌ನಂತೆ ಪರಿಗಣಿಸಿ. ಅದರ ಕುರಿತು ಯಾರಾದರೂ ಹಿಡಿತ ಸಾಧಿಸಿದರೆ, ಅವರು ನಿಮ್ಮ ಸಿಂಕ್ ಮಾಡಲಾದ ಡೇಟಾವನ್ನು ಓದಬಹುದು ಮತ್ತು ಮಾರ್ಪಡಿಸಬಹುದು.</translation>
<translation id="8914601399214979966">ಬದಲಿಗೆ ನನ್ನ ಕ್ಯಾಮೆರಾ ಬಳಸಿ</translation>
<translation id="3034163433136515805">QR ಕೋಡ್ ವೀಕ್ಷಿಸಿ</translation>
<translation id="5093410893656857925">ಸಾಧನ ಪ್ರಕಾರ ಆರಿಸಿ</translation>
<translation id="7900569862982389597">ನೀವು ಸಿಂಕ್ ಮಾಡಲು ಬಯಸುವ ಸಾಧನ ವಿಧವನ್ನು ಆರಿಸಿ.</translation>
<translation id="9017050385815017931">ಫೋನ್/ಟ್ಯಾಬ್ಲೆಟ್</translation>
<translation id="1736420071277903564">ಕಂಪ್ಯೂಟರ್‌</translation>
<translation id="1258250624898033822">ಅಮಾನ್ಯ ಸಿಂಕ್ ಕೋಡ್.</translation>
<translation id="3259723213051400722">ದಯವಿಟ್ಟು ಮತ್ತೆ ಪ್ರಯತ್ನಿಸಿ.</translation>
<translation id="5257281382608458806">ಸಾಧನದ ಹೆಸರು ಅಗತ್ಯವಿದೆ.</translation>
<translation id="8349682220522608879">ದಯವಿಟ್ಟು ಸಿಂಕ್ ಕೋಡ್ ಸೇರಿಸಿ.</translation>
<translation id="1468095392829989299">ಸಿಂಕ್ ಸರ್ವರ್‌ಗಳನ್ನು ಸಂಪರ್ಕಿಸಲಾಗಲಿಲ್ಲ</translation>
<translation id="1399673826422570698">ನೀವು ನೆಟ್‌ವರ್ಕ್ ತೊಂದರೆಯನ್ನು ಹೊಂದಿರಬಹುದು ಅಥವಾ ಸಿಂಕ್ ಸರ್ವರ್‌ಗಳ ನಡುವೆ ಸಮಸ್ಯೆ ಇರಬಹುದು.</translation>
<translation id="5286379413446391050">ಸಿಂಕ್ ಸರ್ವರ್‌ಗಳನ್ನು ಸಂಪರ್ಕಿಸಲಾಗಲಿಲ್ಲ</translation>
<translation id="2410472020237369473">ನಿಮ್ಮ ಕಂಪ್ಯೂಟರ್‌ ಗಡಿಯಾರವನ್ನು ತಪ್ಪಾದ ಸಮಯ ಅಥವಾ ಸಮಯದ ವಲಯಕ್ಕೆ ಹೊಂದಿಸಿರಬಹುದು. ನಿಮ್ಮ ಗಡಿಯಾರ ಸೆಟ್ಟಿಂಗ್‌ಗಳನ್ನು ಪರಿಶೀಲಿಸಿ.</translation>
