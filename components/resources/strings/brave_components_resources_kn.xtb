<translation id="6401272292123354754">Basic Attention Token ಆಧಾರದಲ್ಲಿ Brave Rewards ರಚಿಸಲಾಗಿದೆ. BAT ಕುರಿತು ಇನ್ನಷ್ಟು ತಿಳಿಯಿರಿ</translation>
<translation id="7823359893981882407">ಇಲ್ಲಿ</translation>
<translation id="4483635855837696643">ನಿಮ್ಮ ವಾಲೆಟ್ ಪುನಃಸ್ಥಾಪಿಸಿ</translation>
<translation id="5972677231925916830">ನಿಮ್ಮ ವಾಲೆಟ್ ಪುನಃಸ್ಥಾಪಿಸಲು ನಿಮ್ಮ ಮರುಪ್ರಾಪ್ತಿ ಕೀ ಬಳಸಿ. ಮರುಪ್ರಾಪ್ತಿ ಕೀ ಬಳಸಿಕೊಂಡು ಪುನಃಸ್ಥಾಪಿಸುವುದರಿಂದ, ನಿಮ್ಮ ಪ್ರಸ್ತುತ ವಾಲೆಟ್ ಬದಲಿಸಲಾಗುತ್ತದೆ. ನಿಮ್ಮ ಪ್ರಸ್ತುತ ವಾಲೆಟ್ ಪುನಃಸ್ಥಾಪಿಸುವುದಕ್ಕೂ ಮೊದಲು ಅದನ್ನು ಖಾಲಿ ಮಾಡಲಾಗಿದೆಯೇ ಅಥವಾ ಬ್ಯಾಕಪ್ ಮಾಡಲಾಗಿದೆಯೇ ಎಂದು ಖಚಿತಪಡಿಸಿಕೊಳ್ಳಿ.</translation>
<translation id="3267911153934785912">ನಿಮ್ಮ ಮರುಪ್ರಾಪ್ತಿ ಕೀ ನಮೂದಿಸಿ ಅಥವಾ</translation>
<translation id="3494964159182650590">ಪುರಸ್ಕಾರಗಳ ಸಾರಾಂಶ</translation>
<translation id="63956545637156715">Brave Rewards ಏಕೆ?</translation>
<translation id="1738658589626889850">ಫೈಲ್ ರೂಪದಲ್ಲಿ ಉಳಿಸಿ</translation>
<translation id="259138486959961679">ಎಲ್ಲ {{numItems}} ಐಟಂಗಳನ್ನು ನೋಡಿ</translation>
<translation id="7857284676033058221">ಎಲ್ಲ {{numSites}} ಸೈಟ್‌ಗಳನ್ನು ನೋಡಿ</translation>
<translation id="3536298624341433814">ನನ್ನ ಪುರಸ್ಕಾರ ಧನ ಕಳುಹಿಸಿ</translation>
<translation id="8428213095426709021">ಸೆಟ್ಟಿಂಗ್‌ಗಳು</translation>
<translation id="1792148354504719133">ಸೈಟ್</translation>
<translation id="5944430251223015324">ಸೈಟ್‌ಗಳು</translation>
<translation id="8826007068782654809">ಧನ್ಯವಾದಗಳು</translation>
<translation id="6432779622695508363">ಮೆಚ್ಚುಗೆಯ ಮೇರೆಗೆ ಪುರಸ್ಕಾರ ಧನ</translation>
<translation id="8424370624815730676">Basic Attention Token (BAT)</translation>
<translation id="5808753495166759239">ಬಿಟ್ ಕಾಯಿನ್ (BTC)</translation>
<translation id="4837149400263828922">ಈಥೇರಿಯಂ (ETH)</translation>
<translation id="3324022052501075296">ಲೈಟ್ ಕಾಯಿನ್ (LTC)</translation>
<translation id="4914597931214386232">ಟೋಕನ್ ಅನುದಾನಗಳು ಕ್ಲೈಮ್ ಮಾಡಿರುವುದು</translation>
<translation id="1634828734222219955">ಒಟ್ಟು</translation>
<translation id="3535654229833214438">ವಹಿವಾಟುಗಳು</translation>
<translation id="8987812599455325915">ಇದು ಜಾಹೀರಾತುಗಳು ಮತ್ತು ಸ್ವಯಂಚಾಲಿತ ಕೊಡುಗೆಗಳು ಎರಡನ್ನೂ ಸಕ್ರಿಯಗೊಳಿಸುತ್ತದೆ. ನೀವು ಅವುಗಳನ್ನು ಯಾವಾಗ ಬೇಕಾದರೂ ಪ್ರತ್ಯೇಕವಾಗಿ ಆನ್ ಅಥವಾ ಆಫ್ ಮಾಡಬಹುದು.</translation>
<translation id="1485484147452131556">ಪುರಸ್ಕಾರಗಳನ್ನು ಸಕ್ರಿಯಗೊಳಿಸಿ</translation>
<translation id="2448312741937722512">ವಿಧ</translation>
<translation id="8772672608371898774">Brave ಪರಿಶೀಲಿಸಿದ ಪ್ರಕಾಶಕರು</translation>
<translation id="5050370287908180130">ವಿವರಗಳಿಗಾಗಿ ಮಾಸಿಕ ಹೇಳಿಕೆಯನ್ನು ವೀಕ್ಷಿಸಿ</translation>
<translation id="8615803647903750175">ವಾಲೆಟ್ ಚಟುವಟಿಕೆ</translation>
<translation id="2620624773940447728">ವಾಲೆಟ್ ವಿಳಾಸ</translation>
<translation id="4989277977420164514">ವಾಲೆಟ್ ಬಾಕಿ</translation>
<translation id="1910721550319506122">ಸುಸ್ವಾಗತ!</translation>
<translation id="4452279058754488731">ಈಗಲೇ ಸಂಪಾದನೆಯನ್ನು ಪ್ರಾರಂಭಿಸಿ!</translation>
<translation id="5595664624688492034">ಪುರಸ್ಕಾರಗಳಲ್ಲಿ ಸೇರಿಕೊಳ್ಳಿ</translation>
<translation id="3315406658665721926">ಗೌಪ್ಯತೆ-ಗೌರವಿಸುವ ಜಾಹೀರಾತುಗಳನ್ನು ವೀಕ್ಷಿಸುವ ಮೂಲಕ ನೀವು ಇದೀಗ ಟೋಕನ್‌ಗಳನ್ನು ಸಂಪಾದಿಸಬಹುದು.</translation>
<translation id="8787500234852052352">ಜಾಹೀರಾತುಗಳನ್ನು ವೀಕ್ಷಿಸಿ, ಹಣ ಪಡೆಯಿರಿ ಮತ್ತು ನಿಮ್ಮ ಮೆಚ್ಚಿನ ವಿಷಯ ರಚನೆಕಾರರನ್ನು ಬೆಂಬಲಿಸಲು ಅದನ್ನು ಮುಂದಕ್ಕೆ ಪಾವತಿಸಿ.</translation>
<translation id="318487899513214296">ಏನೆಲ್ಲಾ ಸುಧಾರಣೆಗಳಾಗಿವೆ ಎಂಬುದನ್ನು ಪರಿಶೀಲಿಸಿ</translation>
<translation id="8446884382197647889">ಇನ್ನಷ್ಟು ತಿಳಿಯಿರಿ</translation>
<translation id="4083384190339959182">ನಾವು Brave ಪಾವತಿಗಳಲ್ಲಿ ಅಧಿಕ ಪರಿಷ್ಕರಣೆಗಳನ್ನು ಮಾಡಿದ್ದೇವೆ.</translation>
<translation id="5395851053710553429">ಹೊಸ ಇಂಟರ್ನೆಟ್ ಅನುಭವವನ್ನು ಪಡೆಯಲು ಸಿದ್ಧರಾಗಿ.</translation>
<translation id="2549982302631665116">ಇಂದು, Brave ನಿಮ್ಮನ್ನು ಹೊಸ ಇಂಟರ್ನೆಟ್‌ಗೆ ಸ್ವಾಗತಿಸುತ್ತದೆ. ಇಲ್ಲಿ ನಿಮ್ಮ ಸಮಯವನ್ನು ಗೌರವಿಸಲಾಗುತ್ತದೆ, ನಿಮ್ಮ ವೈಯಕ್ತಿಕ ಮಾಹಿತಿಯನ್ನು ಖಾಸಗಿಯಾಗಿರಿಸಲಾಗುತ್ತದೆ ಮತ್ತು ನಿಮ್ಮ ಗಮನ ಕೇಂದ್ರೀಕರಿಸುವುದಕ್ಕಾಗಿ ನಿಮಗೆ ಹಣವನ್ನು ಪಾವತಿಸಲಾಗುತ್ತದೆ.</translation>
<translation id="3780605330121608425">ನಿಮ್ಮ ವಾಲೆಟ್</translation>
</translationbundle>