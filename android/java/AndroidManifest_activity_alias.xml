<activity-alias
            android:name="org.chromium.chrome.browser.HoloAlias"
            android:enabled="false"
            android:icon="@drawable/ic_launcher_holo"
            android:roundIcon="@drawable/ic_launcher_holo"
            android:label="@string/app_name"
            android:targetActivity="org.chromium.chrome.browser.ChromeTabbedActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity-alias>