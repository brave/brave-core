<?xml version="1.0" encoding="utf-8"?>
<resources>
	<declare-styleable name="brave_list_preference">
        <attr name="dialog_subtitle" format="string"/>
        <attr name="dialog_entries" format="string"/>
        <attr name="dialog_default_index" format="integer"/>
    </declare-styleable>

    <declare-styleable name="BraveAccountPreference">
        <!-- The text color for the preference title. -->
        <attr name="titleTextColor" format="color"/>
        <!-- The text color for the preference summary. -->
        <attr name="summaryTextColor" format="color"/>
        <!-- Whether to make the title text bold. -->
        <attr name="boldTitle" format="boolean"/>
    </declare-styleable>

    <attr name="ic_onboarding_backup" format="reference"/>
    <attr name="ic_onboarding_intro" format="reference"/>
    <attr name="ic_onboarding_password" format="reference"/>
</resources>
