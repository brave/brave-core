diff --git a/chrome/browser/ui/browser_live_tab_context.cc b/chrome/browser/ui/browser_live_tab_context.cc
index 4e9544e113eb3cf69f01a02ce0925f8b560a2eb3..c0d59dd0529d904a3e54b9392dd2355d707505a1 100644
